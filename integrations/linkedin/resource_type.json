{
  "description": "Schema for the resource configuration",
  "schema": {
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "properties": {
      "token": {
        "type": "string",
        "description": "Your access token for linkedin app"
      },
      "apiVersion": {
        "type": "string",
        "description": "API version for the implemented apis, format must be YYYYMM"
      }
    },
    "required": ["token", "apiVersion"]
  }
}